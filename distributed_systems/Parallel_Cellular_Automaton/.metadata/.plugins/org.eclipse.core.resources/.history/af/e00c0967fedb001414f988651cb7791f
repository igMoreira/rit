
public class CA {

	public static void main(String[] args) {

		String updateRule = "01111000";
//		Bit[] initialState = 
		
		
	}
	
	public static int bitSetToInt(BitSet bitSet)
	{
	    int bitInteger = 0;
	    for(int i = 0 ; i < 32; i++)
	        if(bitSet.get(i))
	            bitInteger |= (1 << i);
	    return bitInteger;
	}

}
