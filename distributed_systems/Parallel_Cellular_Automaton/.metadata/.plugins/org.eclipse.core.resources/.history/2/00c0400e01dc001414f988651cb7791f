import java.util.BitSet;


public class Worker {
	private int position
	public BitSet calcNextState(BitSet currentState, int[] updateRule)
	{
		
		if(position == 0)
		{
			// First cell
		}
		else if( position == (currentState.length()-1))
		{
			// last cell
		}
		else
		{
			// cell in the middle
		}
		
		return null;
	}
}
