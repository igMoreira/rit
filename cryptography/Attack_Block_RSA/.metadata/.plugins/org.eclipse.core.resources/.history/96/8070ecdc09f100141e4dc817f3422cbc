public class RsaDecrypt {

	public static void main(String[] args) {
		FileReader file = null;
		try {
			file = new FileReader(args[0]);
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
	}
	
}
